<!-- **FTI Config 3**: Add the reference to the credentialstore.xml as below
-->

<CredentialStore>
	<CredentialStorage type="file" path="<OracleBIData>/web/config/credentialstore.xml" passphrase="another_secret"/>
</CredentialStore>

<!-- End of **FTI Config 3** -->

<!-- **FTI Config 4**: Add the below SSO related information
	a. Enable the SSO with the tag <SSO enabled="true">
	b. Impersonate the BI Server on behalf of the SSO User using:
	   <Param name="IMPERSONATE" source="serverVariable" nameInSource="REMOTE_USER"/>
	c. Provide the LogoffURL and LogonURL using the respective tags.
		Ex: <LogoffUrl>http://myotm.oracle.com</LogoffUrl>
		    <LogonUrl>http://myotm.oracle.com</LogonUrl>
-->
<Auth>
<SSO enabled="true">
<ParamList>
	<Param name="IMPERSONATE" source="serverVariable" nameInSource="REMOTE_USER"/>
</ParamList>
<LogoffUrl>http://{OTM-WEBSERVER}</LogoffUrl>
<LogonUrl>http://{OTM-WEBSERVER}</LogonUrl>
</SSO>
</Auth> 
<!-- End of **FTI Config 4** -->
</ServerInstance>
</WebConfig>
